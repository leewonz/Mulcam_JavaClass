package day06;

abstract class Shape{
	public abstract double getArea();
}

class Point{
	int x;
	int y;
}

interface Drawable{
	void draw();
}

interface Movable{
	void move();
}

public class Circle extends Shape implements Drawable, Movable{

	Point p;
	double r;
	
	public Circle() {
		super();
		this.p = p;
		this.r = r;
	}
	
	@Override
	public void move() {
		System.out.println("move");
		
	}

	@Override
	public void draw() {
		System.out.println("draw");
		
	}

	@Override
	public double getArea() {
		// TODO Auto-generated method stub
		return Math.PI * r * r;
	}
	
}
