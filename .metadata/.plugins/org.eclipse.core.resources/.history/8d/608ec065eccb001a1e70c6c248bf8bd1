package day04;

public class AccountMgr {
	Account[] accounts;
	int count;
	
	public AccountMgr() {
		accounts = new Account[10];
	}
	
	public AccountMgr(int size) {
		accounts = new Account[size];
	}
	
	public void add(Account acc) {
		if (count == accounts.length) {
			
		}
		accounts[count++] = acc;
	}
	public void print() {
		for(int i = 0; i < count; i++) {
			accounts[i].print();
		}
	}
}
