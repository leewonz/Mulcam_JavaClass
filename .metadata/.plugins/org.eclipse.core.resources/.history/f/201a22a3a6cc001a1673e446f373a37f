package day05;

public class Dog extends Animal{
	String kind = "강아지 종류";
	String name;
	
	public Dog() {
		super();
	}

	public Dog(String kind, String name) {
		super();
		this.kind = kind;
		this.name = name;
	}

	public Dog(String skind, String kind, String name) {
		super();
		super.kind = kind;
		this.kind = kind;
		this.name = name;
	}

	public void print() {
		System.out.printf("Dog[%s%s%s]%n",
						  super.kind,
						  this.kind,
						  this.name);
	}
}
