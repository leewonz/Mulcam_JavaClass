package day03;

public class Test01 {

	public static void main(String[] args) {
		
		for(int i = 0; i < 10; i++) {
			System.out.printf("%sHello Java.%d%n", i % 2 == 0 ? "" : " ", i);
		}
		
		System.out.println("---------------");
		
		int sum = 0;
		for(int i = 0; i <= 100; i++) {
			sum += i;
			System.out.printf("%d + %n",i);
		}
		System.out.println(sum);
	}
}
